     <Simulator version="0.1">
          <System>
               <Plugins>
                    <Plugin source="liboperators.so" xsdfile="XSD/Operators.xsd"/>
                    <Plugin source="libcontrol.so" xsdfile="XSD/Control.xsd"/>
                    <Plugin source="libdata.so" xsdfile="XSD/Data.xsd"/>
                    <Plugin source="libmeds.so" xsdfile="XSD/Meds.xsd"/>
               </Plugins>
               <Parameters>
                    <Entry label="ref.cost_test_hiv">
                         <Double value="10.0"/>
                    </Entry>
                    <Entry label="threads.simulator">
                         <UInt value="8"/>
                    </Entry>
                    <Entry label="threads.generator">
                         <UInt value="8"/>
                    </Entry>
                    <Entry label="contacts.algo">
                         <String value="Contacts_Base"/>
                    </Entry>
               </Parameters>
          </System>
          <Input>
               <Environment file="Environment/Environment.xml"/>
               <PopulationManager>
                    <Generator>
                         <RandomizerInfo>
                              <Randomizer seed="1753706839683998249" state=""/>
                              <Randomizer seed="1345008094536364482" state=""/>
                              <Randomizer seed="1676261052021901356" state=""/>
                              <Randomizer seed="360002778458394220" state=""/>
                              <Randomizer seed="38206298792389231" state=""/>
                              <Randomizer seed="905535475467524846" state=""/>
                              <Randomizer seed="215524696581561974" state=""/>
                              <Randomizer seed="1751929966499464495" state=""/>
                         </RandomizerInfo>
                         <Profiles>
                              <GenProfile label="populationTst">
                                   <Demography/>
                                   <AcceptFunction>
                                        <PrimitiveTree>
                                             <Data_Value inValue="true" inValue_Type="Bool"/>
                                        </PrimitiveTree>
                                   </AcceptFunction>
                                   <IndividualModel/>
                                   <SimulationVariables file="Populations/SimulationVariables.xml"/>
                              </GenProfile>
                         </Profiles>
                    </Generator>
                    <Population>
                         <SubPopulation profile="populationTst" size="1000" time="0"/>
                    </Population>
               </PopulationManager>
          </Input>
          <Simulation>
               <RandomizerInfo>
                    <Randomizer seed="959400711787624359" state=""/>
                    <Randomizer seed="187276972963900079" state=""/>
                    <Randomizer seed="326919918461976671" state=""/>
                    <Randomizer seed="311068971953517775" state=""/>
                    <Randomizer seed="1589147721975866572" state=""/>
                    <Randomizer seed="1036475563163453882" state=""/>
                    <Randomizer seed="1533446641699142499" state=""/>
                    <Randomizer seed="394138457246418855" state=""/>
               </RandomizerInfo>
               <Clock units="week">
                    <PrimitiveTree>
                         <Operators_IsEqualComplex>
                              <Data_Clock/>
                              <Data_Value inValue="52" inValue_Type="ULong"/>
                         </Operators_IsEqualComplex>
                    </PrimitiveTree>
               </Clock>
               <Processes>
                    <Process file="Processes/scenario_NoRisk.xml" label="scenario_NoRisk"/>
                    <Process file="Processes/scenario_risk.xml" label="scenario_risk"/>
                    <Process file="Processes/test_HIV.xml" label="test_HIV"/>
                    <Process file="Processes/get_infected.xml" label="get_infected"/>
               </Processes>
               <Scenarios>
                    <Scenario processIndividual="scenario_NoRisk" label="noRisk"/>
                    <Scenario processIndividual="scenario_risk" label="Risk"/>
               </Scenarios>
               <ClockObservers/>
          </Simulation>
          <Output>
               <Environment/>
               <Population>
                    <SubPopulation profile="populationTst">
                         <Variable label="isHIV"/>
                         <Variable label="is_false_negative"/>
                         <Variable label="is_false_positive"/>
                         <Variable label="is_true_negative"/>
                         <Variable label="is_true_positive"/>
                         <Variable label="total_cost"/>
                    </SubPopulation>
               </Population>
          </Output>
     </Simulator>
