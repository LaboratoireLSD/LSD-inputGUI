<?xml version="1.0" encoding="UTF-8"?>
  <Process label="ScreenA">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_IfThenElse>
        <Data_Value inValue="$ref.CheckA"/>
        <Control_Sequential>
          <Control_IfThenElse>
            <!--Singleton add doctor cost-->
            <Data_Value inValue="@Sg2ndGroup1"/>
            <Control_Nothing/>
            <Control_Sequential>
              <Operators_AddVariable inArgRight="$ref.CostDrGroup1" inArgLeft="@Cost" outResult="@Cost"/>
              <Data_SetVariable inValue="true" outVariable="@Sg1stGroup1" inValue_Type="Bool"/>
            </Control_Sequential>
          </Control_IfThenElse>
          <Control_IfThenElse>
            <!--Singleton add 1st cost-->
            <Data_Value inValue="@Sg1stGroup1"/>
            <Control_Nothing/>
            <Control_Sequential>
              <Operators_AddVariable inArgRight="$ref.Cost1stGroup1" inArgLeft="@Cost" outResult="@Cost"/>
              <Data_SetVariable inValue="true" outVariable="@Sg1stGroup1" inValue_Type="Bool"/>
            </Control_Sequential>
          </Control_IfThenElse>
          <Control_Branch inProbability=".99">
            <Data_SetVariable inValue="true" outVariable="@is_haveA" inValue_Type="Bool"/>
            <Control_Sequential>
              <Control_IfThenElse>
                <!--Singleton add 2nd cost-->
                <Data_Value inValue="@Sg2ndGroup1"/>
                <Control_Nothing/>
                <Control_Sequential>
                  <Operators_AddVariable inArgRight="@Sg2ndGroup1" inArgLeft="@Cost" outResult="@Cost"/>
                  <Data_SetVariable inValue="true" outVariable="@Sg2ndGroup1" inValue_Type="Bool"/>
                </Control_Sequential>
              </Control_IfThenElse>
              <Data_SetVariable inValue="true" outVariable="@is_haveA" inValue_Type="Bool"/>
            </Control_Sequential>
          </Control_Branch>
        </Control_Sequential>
        <Control_Nothing/>
      </Control_IfThenElse>
    </PrimitiveTree>
  </Process>
